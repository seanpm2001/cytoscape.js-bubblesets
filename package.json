{
  "name": "cytoscape-bubblesets",
  "description": "Cytoscape.js plugin for rendering bubblesets",
  "version": "3.0.0",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://www.sgratzl.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/upsetjs/cytoscape.js-bubblesets",
  "bugs": {
    "url": "https://github.com/upsetjs/cytoscape.js-bubblesets/issues"
  },
  "keywords": [
    "cytoscape",
    "bubblesets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/upsetjs/cytoscape.js-bubblesets.git"
  },
  "global": "CytoscapeBubbleSets",
  "dependencies": {
    "@types/cytoscape": "^3.14.12"
  },
  "peerDependencies": {
    "cytoscape": "^3.18.1",
    "cytoscape-layers": "^2.1.0"
  },
  "browserslist": [
    "Firefox ESR",
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "module": "build/index.js",
  "main": "build/index.cjs.js",
  "browser": "build/index.umd.js",
  "unpkg": "build/index.umd.min.js",
  "jsdelivr": "build/index.umd.min.js",
  "types": "build/index.d.ts",
  "sideEffects": false,
  "files": [
    "build",
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/jest": "^26.0.20",
    "@types/lodash.throttle": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@yarnpkg/pnpify": "^2.4.0",
    "bubblesets-js": "^2.2.0",
    "cytoscape": "^3.18.1",
    "cytoscape-layers": "^2.1.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "lodash.throttle": "^4.1.1",
    "prettier": "^2.2.1",
    "release-it": "^14.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.2",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.28",
    "typescript": "~4.1.5"
  },
  "scripts": {
    "clean": "rimraf build docs node_modules \"*.tgz\" \"*.tsbuildinfo\" \"samples/*.js\" \"samples/*.map\"",
    "compile": "tsc -b tsconfig.c.json",
    "start": "yarn run watch",
    "watch": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watch",
    "test:coverage": "jest --passWithNoTests --coverage",
    "lint": "yarn run eslint && yarn run prettier",
    "fix": "yarn run eslint:fix && yarn run prettier:write",
    "prettier:write": "prettier \"*\" \"*/**\" --write",
    "prettier": "prettier \"*\" \"*/**\" --check",
    "eslint": "eslint src --ext .ts,.tsx",
    "eslint:fix": "yarn run eslint --fix",
    "docs": "typedoc src/index.ts",
    "prepare": "yarn run build",
    "release": "release-it --disable-metrics --npm.skipChecks",
    "release:pre": "release-it --disable-metrics --npm.skipChecks --preRelease=alpha --npm.tag=next"
  }
}
